{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "lib": ["es2022", "dom"],
    "downlevelIteration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "types": ["cypress", "express", "express-session", "node"],
    "typeRoots": ["../node_modules/@types", "../node_modules", "../server/@types"]
  },
  "include": ["**/*.ts", "../server/@types/**/*.d.ts"]
}
